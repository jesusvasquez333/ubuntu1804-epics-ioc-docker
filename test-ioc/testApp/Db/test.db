record(longin, $(P):id) {
    field(DESC, "IOC ID")
    field(VAL, $(ID))
    field(PINI, "YES")
}

record(calcout, $(P):upTimeCnt) {
    field(DESC, "Uptime counter")
    field(EGU, "s")
    field(SCAN, "1 second")
    field(A, 0)
    field(CALC, "A+1")
    field(OUT, "$(P):upTimeCnt.A")
}

record(waveform, ${P}:signal) {
    field(DESC, "Random signal")
    field(NELM,  100)
    field(FTVL, "LONG")
    field(SCAN, "Passive")
}

record(aSub,    $(P):sigGen) {
    field(SNAM, "signalGenerator") 
    field(INPA, "$(P):id")
    field(OUTA, "$(P):signal PP") 
    field(FTVA, "LONG")
    field(NOVA, "100")
    field(SCAN, "1 second") 
}
